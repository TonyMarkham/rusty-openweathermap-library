{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "lon": {
      "type": "number",
      "description": "Longitude coordinate",
      "minimum": -180,
      "maximum": 180
    },
    "lat": {
      "type": "number",
      "description": "Latitude coordinate",
      "minimum": -90,
      "maximum": 90
    }
  },
  "required": ["All"]
}
